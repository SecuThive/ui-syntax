generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Component {
  id          Int     @id @default(autoincrement())
  category    String
  name        String
  description String
  code        String
  
  @@unique([category, name], name: "category_name")
}
